  },
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "clean": "rm -rf ./dist ./coverage ./log ./build",
    "compile": "tsc",
    "compile:dev": "tsc -w",
    "coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' 'tests/**/*.{js,ts,tsx}' --quiet",
    "full-lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --quiet --fix && prettier --write 'src/**/*.{js,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}' 'tests/**/*.{js,ts,tsx}'",
    "test": "jest --clearCache && jest --runInBand --verbose false",
    "test:e2e": "jest --clearCache && jest --config ./tests/jest-e2e.json --runInBand --verbose false",
    "test:all": "jest --clearCache && jest --config ./jest-all.json --runInBand --verbose false",
    "clearCache": "jest --clearCache"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.38",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.25.0",
