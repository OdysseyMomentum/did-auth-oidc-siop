    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.5.4",
    "jest-sonar": "^0.2.7",
    "jose": "^1.27.0",
    "lint-staged": "^10.1.3",
    "moment": "^2.25.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "ts-loader": "^7.0.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@validated-id/vid-did-resolver": "0.0.7",
    "axios": "^0.19.2",
    "did-jwt": "^4.3.2",
    "did-resolver": "^1.1.0",
